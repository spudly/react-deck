{
  "name": "react-deck",
  "description": "Render a deck of cards with react",
  "main": "lib/Deck.js",
  "scripts": {
    "test": "npm run lint && nyc --require babel-register mocha -u exports",
    "build": "babel src --out-dir lib",
    "build:demo": "browserify -t [ babelify --presets [ es2015 react stage-0 ] ] demo/demo.js > demo/demo_build.js",
    "prepublish": "npm run build",
    "lint": "lint src test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spudly/react-deck.git"
  },
  "keywords": [
    "react",
    "component",
    "deck",
    "cards"
  ],
  "author": "Stephen Sorensen <shuoink@gmail.com> (http://www.stephenjohnsorensen.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spudly/react-deck/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "dependencies": {
    "classnames": "^2.1.2",
    "react-card": "^2.0.0"
  },
  "devDependencies": {
    "@spudly/eslint-config": "^3.0.0",
    "assert-diff": "^1.0.1",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "chai": "^4.0.0",
    "mocha": "^3.0.0",
    "nyc": "^11.0.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "semantic-release": "^6.3.6"
  },
  "peerDependencies": {
    "react": "^15.2.1"
  },
  "files": [
    "lib"
  ],
  "eslintConfig": {
    "extends": "@spudly"
  }
}
